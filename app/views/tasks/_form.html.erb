<%= simple_form_for(@task) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :duration %>
  <%= f.input :priority, collection: ['!', '!!', '!!!'], input_html: { class: 'selectpicker' }, prompt: "Choose From List" %>
  <%= f.input :date %>
  <%= f.input :start_time %>
  <%= f.input :completed %>
  <%= f.button :submit %>
<% end %>

